<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CustomViewClock">
        <attr name="stroke_width" format="dimension"/>
        <attr name="stroke_color" format="color"/>
        <attr name="hour_hand_width" format="dimension"/>
        <attr name="hour_hand_color" format="color"/>
        <attr name="minute_hand_width" format="dimension"/>
        <attr name="minute_hand_color" format="color"/>
        <attr name="second_hand_width" format="dimension"/>
        <attr name="second_hand_color" format="color"/>
        <attr name="number_text_size" format="dimension" />
        <attr name="number_color" format="color" />
        <attr name="is_draw_hour_number" format="boolean"/>
        <attr name="is_draw_minute" format="boolean" />
        <attr name="is_draw_second_hand" format="boolean"/>
        <attr name="time" format="string" />
        <attr name="is_tictac" format="boolean" />
    </declare-styleable>
</resources>